{
    "version": 2,
    "builds": [
        {
            "src": "app/__init__.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "app/__init__.py"
        },
        {
            "src": "/(.*)",
            "dest": "app/__init__.py"
        }
    ]
}